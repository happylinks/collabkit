    "prepublishOnly": "yarn build",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "build-types": "vue-tsc && rollup --config rollup.config.js"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@collabkit/theme": "^0.0.0",
    "@stitches/core": "^1.2.8",
    "@types/node": "^16.11.47",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.1.0",
    "date-fns": "^2.28.0",
    "lexical": "0.3.9",
    "lexical-vue": "^0.1.7",
    "npm-run-all": "^4.1.5",
    "phosphor-vue": "^4.4.0",
    "rimraf": "^3.0.2",
    "rollup": "2.77.3",
    "rollup-plugin-dts": "^4.2.2",
    "typescript": "~4.7.4",
    "vite": "^3.0.4",
    "vue": "^3.2.37",
    "vue-tsc": "^0.39.5"
  }
}
