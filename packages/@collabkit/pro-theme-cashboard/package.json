{
  "name": "@collabkit/pro-theme-cashboard",
  "version": "0.6.3",
  "type": "module",
  "files": [
    "dist/collabkit-react.d.ts",
    "dist/collabkit-react.js",
    "dist/collabkit-react.umd.cjs",
    "dist/style.css"
  ],
  "types": "./dist/collabkit-react.d.ts",
  "main": "./dist/collabkit-react.umd.cjs",
  "module": "./dist/collabkit-react.js",
  "exports": {
    "./dist/style.css": "./dist/style.css",
    ".": {
      "types": "./dist/collabkit-react.d.ts",
      "import": "./dist/collabkit-react.js",
      "require": "./dist/collabkit-react.umd.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite dev",
    "build": "yarn clean && tsc && vite build && rollup --config rollup.config.js",
    "clean": "rimraf dist types *.tsbuildinfo src/**/*.js",
    "preview": "vite preview",
    "prepublishOnly": "yarn build",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@collabkit/react": "*",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.19",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "2.77.3",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-visualizer": "^5.8.0",
    "tailwindcss": "^3.2.4",
    "vite": "^3.0.9"
  },
  "prettier": {
    "singleQuote": true
  }
}
